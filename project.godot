; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GoMavSimulator"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Notification="*res://modules/notice/Notification.gd"
GeneralSettings="*res://modules/settings/general/general_settings.gd"
MavlinkSettings="*res://modules/settings/mavlink/mavlink_settings.gd"
SimulatorSettings="*res://modules/settings/simulator/simulator_settings.gd"
VehicleFactory="*res://modules/vehicle/VehicleFactory.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="GoMavSimulator"
