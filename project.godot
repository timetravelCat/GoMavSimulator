; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GoMavSimulator"
config/tags=PackedStringArray("dev")
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Notification="*res://modules/notice/Notification.tscn"
GeneralSettings="*res://modules/settings/general/GeneralSettings.tscn"
GraphicsSettings="*res://modules/settings/general/GraphicsSettings.tscn"
MavlinkSettings="*res://modules/settings/mavlink/MavlinkSettings.tscn"
SimulatorSettings="*res://modules/settings/simulator/SimulatorSettings.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="GoMavSimulator"

